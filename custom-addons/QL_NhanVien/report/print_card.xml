<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="print_employee_badge">
        <t t-call="web.html_container">
            <!-- Sửa lại cú pháp t-foreach và t-as -->
            <t t-foreach="docs" t-as="doc">
                <!-- Đảm bảo rằng template này tồn tại -->
                <t t-call="ql_nhanvien.print_employee_badge"/>
            </t>
        </t>
    </template>
</odoo>
